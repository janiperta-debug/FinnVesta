{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "brain": ["./brain/index.ts"],
      "brain/*": ["./brain/*"],
      "types": ["./apiclient/data-contracts.ts"],
      "components/*": ["./components/*"],
      "pages/*": ["./pages/*"],
      "app": ["./app"],
      "app/*": ["./app/*"],
      "utils/*": ["./utils/*"],
      "@/*": ["./*"],
      "@/hooks/*": ["./extensions/shadcn/hooks/*"],
      "@/components/hooks/*": ["./extensions/shadcn/hooks/*"],
      "@/components/ui/*": ["./extensions/shadcn/components/*"]
    }
  },
  "include": ["env.d.ts", "."],
  "exclude": [
    "brain/Brain.ts",
    "brain/BrainRoute.ts",
    "brain/data-contracts.ts",
    "brain/http-client.ts",
    "apiclient/Apiclient.ts",
    "apiclient/ApiclientRoute.ts",
    "apiclient/data-contracts.ts",
    "apiclient/http-client.ts",
    "node_modules"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}
